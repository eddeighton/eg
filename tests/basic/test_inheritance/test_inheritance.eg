TEST( (const char*)ev.value == "A::D"s );
while( events::get( iter, ev ) && ( ev.type != "log"s ) );
TEST( (const char*)ev.value == "A::D::G"s );
while( events::get( iter, ev ) && ( ev.type != "log"s ) );
TEST( (const char*)ev.value == "E"s );
while( events::get( iter, ev ) && ( ev.type != "log"s ) );
